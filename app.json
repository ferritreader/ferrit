{
  "name": "libbacon",
  "description": "Private front-end for Reddit",
  "buildpacks": [
    {
      "url": "https://github.com/emk/heroku-buildpack-rust"
    },
    {
      "url": "emk/rust"
    }
  ],
  "stack": "container",
  "env": {
    "LIBBACON_DEFAULT_THEME": {
      "required": false
    },
    "LIBBACON_DEFAULT_FRONT_PAGE": {
      "required": false
    },
    "LIBBACON_DEFAULT_LAYOUT": {
      "required": false
    },
    "LIBBACON_DEFAULT_WIDE": {
      "required": false
    },
    "LIBBACON_DEFAULT_COMMENT_SORT": {
      "required": false
    },
    "LIBBACON_DEFAULT_POST_SORT": {
      "required": false
    },
    "LIBBACON_DEFAULT_SHOW_NSFW": {
      "required": false
    },
    "LIBBACON_DEFAULT_BLUR_NSFW": {
      "required": false
    },
    "LIBBACON_USE_HLS": {
      "required": false
    },
    "LIBBACON_HIDE_HLS_NOTIFICATION": {
      "required": false
    }
  }
}
